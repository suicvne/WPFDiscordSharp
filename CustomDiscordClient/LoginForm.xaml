<Window x:Class="CustomDiscordClient.LoginForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomDiscordClient"
        mc:Ignorable="d"
        Title="Login to Discord" Height="144.803" Width="387.991" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="295,81,0,0" IsCancel="True" Click="cancelButton_Click"/>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="215,81,0,0" Click="loginButton_Click"/>
        <Label x:Name="label" Content="Email" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,13,0,0" Width="85" Height="26"/>
        <Label x:Name="label1" Content="Password&#xD;&#xA;" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,39,0,0" Width="85" Height="32"/>
        <TextBox x:Name="emailTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="270" Margin="100,14,0,0"/>
        <PasswordBox x:Name="passwordTextBox" HorizontalAlignment="Left" Margin="100,42,0,0" VerticalAlignment="Top" Width="270"/>

    </Grid>
</Window>
